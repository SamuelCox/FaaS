@using FaaS.Models
@model Settings
@{
    ViewData["Title"] = "Settings";
}

<h2>Settings</h2>


<div>
        @{
            foreach(string x in Model.Connections) {
                <div>
                    <input type="text" value="@x" />
                </div>
            }
        }
        <div>
            <input type="text" id="newConnection" />
            <button onclick="addConnection(document.getElementById('newConnection')">Save </button>
        </div>
</div>
